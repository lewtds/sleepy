#: ../src/trayicon.vala:9
msgid "Sleep Timer Indicator"
msgstr "Sleep Timer Indicator"

#. Note: We are building the menu upward
#: ../src/trayicon.vala:30
msgid "Stop"
msgstr "Dừng lại"

#. Remaining time
#. Update the indicator status, icon,etc...
#: ../src/trayicon.vala:43 ../src/trayicon.vala:108
#, c-format
msgid "%s left"
msgstr "Còn %s nữa"

#: ../src/trayicon.vala:59
msgid "Set Timer"
msgstr "Hẹn giờ"

#: ../src/trayicon.vala:66
msgid "6:00 AM"
msgstr "6:00 AM"

#: ../src/trayicon.vala:74
msgid "6:30 AM"
msgstr "6:30 AM"

#: ../src/trayicon.vala:82
msgid "7:00 AM"
msgstr "7:00 AM"

#: ../src/trayicon.vala:98
msgid "Quit"
msgstr "Thoát"

#: ../src/trayicon.vala:134
msgid "The clock is ticking..."
msgstr "Đồng hồ đang chạy..."

#: ../src/trayicon.vala:135
#, c-format
msgid "And it will go off at %s. You have %s left, have fun :-j"
msgstr "Và sẽ đổ chuông lúc %s. Bạn còn %s nữa, cứ thoải mái đi :-j"

#: ../src/trayicon.vala:147
msgid "The clock has stopped!"
msgstr "Đồng hồ đã ngừng chạy!"

#: ../src/trayicon.vala:164
msgid "30 minutes left..."
msgstr "Còn 30 phút nữa..."

#: ../src/trayicon.vala:164
msgid "Take your time. :-j"
msgstr "Cứ từ từ. :-j"

#: ../src/trayicon.vala:168
msgid "15 minutes left..."
msgstr "Còn 15 phút nữa..."

#: ../src/trayicon.vala:168
msgid "Maybe preparing the bed now is not a bad idea. ;)"
msgstr "Có lẽ bây giờ dọn giường là vừa đấy. ;)"

#: ../src/trayicon.vala:172
msgid "5 minutes left..."
msgstr "Còn 5 phút nữa..."

#: ../src/trayicon.vala:172
msgid "Please save your work and get ready for bed. ZzzZzz"
msgstr "Thu xếp công việc rồi đi ngủ nào. ZzzZzz"

#: ../src/utility.vala:53
#, c-format
msgid "%d hour"
msgid_plural "%d hours"
msgstr[0] "%d giờ"

#: ../src/utility.vala:54
#, c-format
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d phút"

#: ../data/sleepy.desktop.in.h:1
msgid "Sleep Timer"
msgstr "Hẹn giờ đi ngủ"

#: ../data/sleepy.desktop.in.h:2
msgid "Tell you the best time to go to sleep"
msgstr "Cho bạn biết khi nào nên đi ngủ để tỉnh dậy thật sảng khoái"

